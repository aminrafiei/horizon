{
    "name": "aminrafiei/horizon",
    "description": "Dashboard and code-driven configuration for Laravel queues.",
    "keywords": ["horizon", "queue"],
    "license": "MIT",
    "authors": [
        {
            "name": "Amin Rafiei",
            "email": "amin.amin111@yahoo.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "cakephp/chronos": "^1.0",
        "illuminate/contracts": "~5.4|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/queue": "~5.4|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/support": "~5.4|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.5",
        "symfony/debug": "~3.3|~4.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "orchestra/database": "^3.5",
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Horizon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Horizon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Aminrafiei\\Horizon\\HorizonServiceProvider"
            ],
            "aliases": {
                "Horizon": "Aminrafiei\\Horizon\\Horizon"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
